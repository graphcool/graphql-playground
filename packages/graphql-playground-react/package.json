{
  "name": "graphql-playground-react",
  "version": "1.7.20",
  "description": "GraphQL IDE for better development workflows (GraphQL Subscriptions, interactive docs & collaboration).",
  "repository": {
    "type": "git",
    "url": "http://github.com/graphcool/graphql-playground.git"
  },
  "license": "MIT",
  "contributors": [
    "Tim Suchanek <tim@graph.cool>",
    "Johannes Schickling <johannes@graph.cool>",
    "Lukas Huvar <lukas@huvar.cz>"
  ],
  "files": [
    "build",
    "lib",
    "playground.css"
  ],
  "main": "./lib/lib.js",
  "typings": "./lib/lib.d.ts",
  "scripts": {
    "build": "rimraf dist build lib && yarn build-app && yarn build-package",
    "build-app": "node scripts/build.js",
    "build-package": "rimraf lib build/dist && npm run tsc && NODE_ENV=production babel --no-babelrc lib -d lib --presets=@babel/preset-env,@babel/preset-react --plugins=babel-plugin-styled-components && cp -r ./src/assets/ ./lib/assets/ && cd lib && rimraf *.jsx;",
    "bump": "npm version patch --no-git-tag-version && git add package.json",
    "bundlesize": "bundlesize",
    "graphql-faker": "graphql-faker ./tests/schema.faker.graphql",
    "lint": "eslint \"src/**/*.ts{,x}\"",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn build",
    "prettier": "prettier --single-quote --no-semi --trailing-comma all --write *.{ts,tsx} 'src/**/*.{ts,tsx}'",
    "start": "node scripts/start.js",
    "start:analyze": "ANALYZE_BUNDLE=true node scripts/start.js",
    "start:graphql-bin": "ACTIVE_APP=graphqlbin node scripts/start.js",
    "test": "node scripts/test.js",
    "tsc": "tsc -p tsconfig.build.json"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --single-quote --no-semi --write",
      "eslint",
      "git add"
    ],
    "gitDir": "../../"
  },
  "babel": {
    "plugins": [
      "babel-plugin-styled-components"
    ],
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "modulePaths": [],
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js",
      "<rootDir>/src/setupEnzyme.ts"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "@hapi/cryptiles": "^4.2.1",
    "@types/lru-cache": "^5.1.0",
    "apollo-link": "^1.0.7",
    "apollo-link-http": "^1.3.2",
    "apollo-link-ws": "1.0.19",
    "calculate-size": "^1.1.1",
    "codemirror": "^5.38.0",
    "codemirror-graphql": "^0.11.6",
    "copy-to-clipboard": "^3.0.8",
    "cuid": "^2.1.8",
    "graphiql": "^0.17.5",
    "graphql": "^14.5.8",
    "immutable": "^4.0.0-rc.9",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.10.0",
    "json-stable-stringify": "^1.0.1",
    "keycode": "^2.1.9",
    "lodash": "^4.17.11",
    "lodash.debounce": "^4.0.8",
    "markdown-it": "^10.0.0",
    "marked": "^0.8.0",
    "prop-types": "^15.6.0",
    "query-string": "6.9.0",
    "react": "^16.12.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-codemirror": "^1.0.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-display-name": "^0.2.3",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.0",
    "react-input-autosize": "^2.2.1",
    "react-modal": "^3.1.11",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-sortable-hoc": "^1.10.1",
    "react-transition-group": "^4.3.0",
    "react-virtualized": "^9.12.0",
    "redux": "^4.0.4",
    "redux-actions": "^2.2.1",
    "redux-immutable": "^4.0.0",
    "redux-localstorage": "rc",
    "redux-localstorage-debounce": "^0.1.0",
    "redux-localstorage-filter": "^0.1.1",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.0.1",
    "styled-components": "^4.0.0",
    "subscriptions-transport-ws": "^0.9.5",
    "utility-types": "^3.10.0",
    "zen-observable": "^0.8.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@svgr/webpack": "4.3.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/deasync": "0.1.0",
    "@types/jest": "24.0.24",
    "@types/node": "12.12.21",
    "@types/react": "16.9.17",
    "@types/zen-observable": "^0.8.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.5",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-react-app": "^9.1.0",
    "bundlesize": "^0.18.0",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "3.0.0",
    "connect-history-api-fallback": "1.6.0",
    "cross-spawn": "7.0.1",
    "css-loader": "3.2.0",
    "detect-port": "1.3.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "4.3.0",
    "filesize": "6.0.1",
    "fs-extra": "^8.1.0",
    "graphql-playground-html": "1.6.13",
    "gzip-size": "5.1.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "http-proxy-middleware": "0.20.0",
    "husky": "3.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-localstorage-mock": "^2.2.0",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "json-loader": "0.5.7",
    "lint-staged": "9.5.0",
    "mini-css-extract-plugin": "0.8.0",
